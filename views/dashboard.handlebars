<header>
    <div class="wrapper">
      <nav>
        <div class="nav-wrapper light-green darken-3">
          <a href="#" class="brand-logo left">TECH BLOG</a>
          <ul id="nav-mobile" class="right">
            <li><a href="/">HOME</a></li>
            <li><a href="#" id="logout-link">LOGOUT</a></li>
          </ul>
        </div>
      </nav>
    </div>
  </header>

<header>
  <h1><a href="/dashboard">Your Dashboard</a></h1>
</header>

<main>
  <h4 class="center">View All Posts</h4>
  <input type="hidden" name="post-id" value="{{post.id}}" />
  {{#each posts as |post| }}
  <div class="divider"></div>
  <div class="section">
    <h5>{{post.title}}</h5>
    <p>{{post.content}}</p>
    <p>Posted By: {{post.user_id}} on {{format_date createdAt}}</p>
  </div>
  {{/each}}
</main>


<a href="/dashboard/new-post">New Post</a>
<script src="/js/logout.js"></script>