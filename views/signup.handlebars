<header>
  <div class="wrapper">
    <nav>
      <div class="nav-wrapper light-green darken-3">
        <a href="#" class="brand-logo left">TECH BLOG</a>
        <ul id="nav-mobile" class="right">
          <li><a href="/">HOME</a></li>
          <li><a href="/dashboard">DASHBOARD</a></li>
          <li><a href="/login">LOGIN</a></li>
        </ul>
      </div>
    </nav>
  </div>
</header>

<div class="row">
  <form class="col s12 signup-form">
    <div class="row">
      <div class="card blue-grey darken-1">
        <div class="card-content white-text">
          <span class="card-title">SIGN UP</span>
          <div class="row">
            <div class="input-field col s12">
              <input id="name-signup" class="validate" />
              <label class="active" for="name">name</label>
            </div>
          </div>
          <div class="row">
            <div class="input-field col s12">
              <input id="username-signup" class="validate" />
              <label class="active" for="username">Username</label>
            </div>
          </div>
          <div class="row">
            <div class="input-field col s12">
              <input id="password-signup" type="password" class="validate" />
              <label class="active" for="password">Password</label>
            </div>
          </div>
          <div class="row">
            <div class="input-field col s12">
              <input id="email-signup" type="email" class="validate" />
              <label class="active" for="email">Email</label>
            </div>
          </div>
          <button class="btn waves-effect waves-light submit" type="submit" name="action" id="submit">Submit
            <i class="material-icons right"></i>
          </button>
        </div>
      </div>
    </div>
  </form>
  <div class="row center-align">
    <a href="/login" class="btn waves-effect waves-teal">
      <h6>Login Instead</h6>
    </a>
  </div>
</div>
<script src="/js/signup.js"></script>